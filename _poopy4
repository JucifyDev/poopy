def show_enemy_info(enemy):
    print("--- Enemy Info ---")
    print("Name:", enemy.name)
    print("Level:", enemy.level)
    print("Health:", enemy.health)
    print("Attack Power:", enemy.attack_power)
    print("Defense Power:", enemy.defense_power)

def battle(player, enemy):
    print(f"A wild {enemy.name} appears!")

    while player.is_alive() and enemy.is_alive():
        show_battle_menu(player, enemy)
        choice = int(input("Select an action: "))
        
        if choice == 1:  # Attack
            player.attack(enemy)
        elif choice == 2:  # Cast Spell
            spell = choose_spell(player)
            if spell:
                player.cast_spell(spell, enemy)
        elif choice == 3:  # Use Item
            show_inventory_menu(player)
            inventory_choice = int(input("Enter your choice: "))
            if inventory_choice == 1:
                equip_weapon(player)
            elif inventory_choice == 2:
                equip_armor(player)
            elif inventory_choice == 3:
                use_item(player)
            elif inventory_choice == 4:
                unequip_weapon(player)
            elif inventory_choice == 5:
                unequip_armor(player)
            elif inventory_choice == 6:
                continue
            else:
                print("Invalid choice.")
        elif choice == 4:  # Run Away
            print("You ran away from the battle.")
            return
        elif choice == 5:  # Save Game
            create_save(player)
        elif choice == 6:  # Quit Game
            quit_game()
        else:
            print("Invalid choice.")
        
        if enemy.is_alive():
            enemy.attack(player)
        
        if not player.is_alive():
            print("You were defeated. Game over.")
            return

    player.gain_experience(enemy.level * 10)
    print("You defeated the", enemy.name, "and earned", enemy.level * 10, "experience points.")

def equip_weapon(player):
    if len(player.inventory) == 0:
        print("You don't have any weapons in your inventory.")
        return
    print("Choose a weapon to equip:")
    for i, weapon in enumerate(player.inventory):
        if isinstance(weapon, Weapon):
            print(i + 1, "-", weapon.name)
    choice = int(input("Enter the number of the weapon to equip: "))
    if choice > 0 and choice <= len(player.inventory):
        weapon = player.inventory[choice - 1]
        if isinstance(weapon, Weapon):
            if player.equipped_weapon is not None:
                player.inventory.append(player.equipped_weapon)
            player.equipped_weapon = weapon
            player.inventory.remove(weapon)
            print("You equipped", weapon.name)
        else:
            print("Invalid choice.")
    else:
        print("Invalid choice.")

def equip_armor(player):
    if len(player.inventory) == 0:
        print("You don't have any armor in your inventory.")
        return
    print("Choose an armor to equip:")
    for i, armor in enumerate(player.inventory):
        if isinstance(armor, Armor):
            print(i + 1, "-", armor.name)
    choice = int(input("Enter the number of the armor to equip: "))
    if choice > 0 and choice <= len(player.inventory):
        armor = player.inventory[choice - 1]
        if isinstance(armor, Armor):
            if player.equipped_armor is not None:
                player.inventory.append(player.equipped_armor)
            player.equipped_armor = armor
            player.inventory.remove(armor)
            print("You equipped", armor.name)
        else:
            print("Invalid choice.")
    else:
        print("Invalid choice.")

def use_item(player):
    if len(player.inventory) == 0:
        print("You don't have any items in your inventory.")
        return
    print("Choose an item to use:")
    for i, item in enumerate(player.inventory):
        if isinstance(item, Item):
            print(i + 1, "-", item.name)
    choice = int(input("Enter the number of the item to use: "))
    if choice > 0 and choice <= len(player.inventory):
        item = player.inventory[choice - 1]
        if isinstance(item, Item):
            player.use_item(item)
        else:
            print("Invalid choice.")
    else:
        print("Invalid choice.")

def unequip_weapon(player):
    if player.equipped_weapon is None:
        print("You don't have any weapon equipped.")
        return
    player.inventory.append(player.equipped_weapon)
    print("You unequipped", player.equipped_weapon.name)
    player.equipped_weapon = None

def unequip_armor(player):
    if player.equipped_armor is None:
        print("You don't have any armor equipped.")
        return
    player.inventory.append(player.equipped_armor)
    print("You unequipped", player.equipped_armor.name)
    player.equipped_armor = None

def equip_title(player):
    if len(player.inventory) == 0:
        print("You don't have any titles in your inventory.")
        return
    print("Choose a title to equip:")
    for i, title in enumerate(player.inventory):
        if isinstance(title, Title):
            print(i + 1, "-", title.name)
    choice = int(input("Enter the number of the title to equip: "))
    if choice > 0 and choice <= len(player.inventory):
        title = player.inventory[choice - 1]
        if isinstance(title, Title):
            if player.equipped_title is not None:
                player.inventory.append(player.equipped_title)
            player.equipped_title = title
            player.inventory.remove(title)
            print("You equipped the", title.name, "title.")
        else:
            print("Invalid choice.")
    else:
        print("Invalid choice.")

def unequip_title(player):
    if player.equipped_title is None:
        print("You don't have any title equipped.")
        return
    player.inventory.append(player.equipped_title)
    print("You unequipped the", player.equipped_title.name, "title.")
    player.equipped_title = None

def quit_game():
    choice = input("Are you sure you want to quit the game? (Y/N): ")
    if choice.upper() == "Y":
        print("Thank you for playing Granjohiem Online. Goodbye!")
        quit()

def play_game():
    global enemies
    player = None
    while True:
        show_main_menu()
        choice = int(input("Enter your choice: "))
        if choice == 1:
            player = start_new_game()
        elif choice == 2:
            player = load_game()
        elif choice == 3:
            quit_game()
        else:
            print("Invalid choice.")
            continue

        while player.hearts > 0:
            print("\n--- Granjohiem Online ---")
            show_stats_menu(player)
            stat_choice = int(input("Enter your choice: "))
            if stat_choice == 1:
                show_enemy_info(enemies[0])
            elif stat_choice == 2:
                show_inventory_menu(player)
                inventory_choice = int(input("Enter your choice: "))
                if inventory_choice == 1:
                    equip_weapon(player)
                elif inventory_choice == 2:
                    equip_armor(player)
                elif inventory_choice == 3:
                    use_item(player)
                elif inventory_choice == 4:
                    unequip_weapon(player)
                elif inventory_choice == 5:
                    unequip_armor(player)
                elif inventory_choice == 6:
                    continue
                else:
                    print("Invalid choice.")
            elif stat_choice == 3:
                show_title_menu(player)
                title_choice = int(input("Enter your choice: "))
                if title_choice == 1:
                    equip_title(player)
                elif title_choice == 2:
                    unequip_title(player)
                elif title_choice == 3:
                    continue
                else:
                    print("Invalid choice.")
            elif stat_choice == 4:
                break
            else:
                print("Invalid choice.")

            enemy = enemies[0]
            battle(player, enemy)
            if player.hearts > 0:
                player.level_up()

def main():
    print("--- Welcome to Granjohiem Online ---")
    play_game()

if __name__ == "__main__":
    main()
