import random
import pickle

magics = [
    # Fire Magic
    ["Fire Magic", "Fireball", "Inferno", "Holy Flame"],
    # Water Magic
    ["Water Magic", "Droplet", "Tsunami", "Aura"],
    # Earth Magic
    ["Earth Magic", "Boulder", "Earthquake", "Earth Wall"],
    # Air Magic
    ["Air Magic", "Gust", "Tornado", "Willowisp"],
    # Admin Magic
    ["Admin Magic", "Holy Thunder", "Eternal Hellflame", "God's Will"]
]

# Player class
class Player:
    def __init__(self, name, race, magic_type):
        self.name = name
        self.race = race
        self.magic_type = magic_type
        self.level = 1
        self.exp = 0
        self.stat_points = 0
        self.health = 100
        self.mana = 100
        self.stamina = 100
        self.damage = 10
        self.defense = 1
        self.magic_power = 1
        self.endurance = 1
        self.hearts = 3
        self.equipped_weapon = None
        self.equipped_armor = None
        self.equipped_title = None
        self.inventory = []
    def is_alive(self):
        return self.health > 0
# Spell data
# Format: [Spell Name, Damage, Mana Cost]
fire_spells = [
    ["Fireball", 20, 15],
    ["Inferno", 40, 30],
    ["Holy Flame", 60, 45]
]

water_spells = [
    ["Droplet", 15, 10],
    ["Tsunami", 30, 20],
    ["Aura", 50, 35]
]

earth_spells = [
    ["Boulder", 10, 5],
    ["Earthquake", 25, 15],
    ["Earth Wall", 45, 30]
]

air_spells = [
    ["Gust", 15, 10],
    ["Tornado", 35, 25],
    ["Willowisp", 55, 40]
]

admin_spells = [
    ["Holy Thunder", 100, 75],
    ["Eternal Hellflame", 120, 90],
    ["God's Will", 150, 110]
]

# Magic data
magics = [
    # Fire Magic
    ["Fire Magic", fire_spells],
    # Water Magic
    ["Water Magic", water_spells],
    # Earth Magic
    ["Earth Magic", earth_spells],
    # Air Magic
    ["Air Magic", air_spells],
    # Admin Magic
    ["Admin Magic", admin_spells]
]

def level_up(self):
        self.level += 1
        self.stat_points += 3
        self.health += 10
        self.mana += 10
        self.stamina += 10
        self.damage += 5
        self.defense += 2
        self.magic_power += 5
        self.endurance += 2

def allocate_stat_points(self):
        print("You have", self.stat_points, "stat points to allocate.")
        print("1. Health (+10)")
        print("2. Mana (+10)")
        print("3. Stamina (+10)")
        print("4. Damage (+5)")
        print("5. Defense (+2)")
        print("6. Magic Power (+5)")
        print("7. Endurance (+2)")
        stat_choice = int(input("Enter the number of the stat you want to allocate points to: "))
        if stat_choice == 1:
            self.health += 10
        elif stat_choice == 2:
            self.mana += 10
        elif stat_choice == 3:
            self.stamina += 10
        elif stat_choice == 4:
            self.damage += 5
        elif stat_choice == 5:
            self.defense += 2
        elif stat_choice == 6:
            self.magic_power += 5
        elif stat_choice == 7:
            self.endurance += 2
        else:
            print("Invalid choice.")

def attack(self, enemy):
        if self.equipped_weapon is not None:
            damage = self.damage + self.equipped_weapon.damage
        else:
            damage = self.damage
        print("You attack the", enemy.name, "and deal", damage, "damage.")
        enemy.take_damage(damage)

def cast_spell(self, enemy):
        magic_spells = magics[self.magic_type][1]
        if self.level < len(magic_spells):
            spell = magic_spells[self.level - 1]
            spell_name = spell[0]
            spell_damage = spell[1]
            spell_mana_cost = spell[2]
            if self.mana >= spell_mana_cost:
                print("You cast", spell_name, "on the", enemy.name, "and deal", spell_damage, "damage.")
                enemy.take_damage(spell_damage)
                self.mana -= spell_mana_cost
            else:
                print("Not enough mana to cast the spell.")
        else:
            print("You don't have a spell for your current level.")

def take_damage(self, damage):
        if self.equipped_armor is not None:
            defense = self.defense + self.equipped_armor.defense
        else:
            defense = self.defense
        damage_taken = max(damage - defense, 0)
        self.health -= damage_taken
        print("You take", damage_taken, "damage.")

def use_item(self, item):
        if item in self.inventory:
            if item == "Health Potion":
                self.health += 20
                if self.health > 100:
                    self.health = 100
                self.inventory.remove(item)
                print("You drink a Health Potion and restore 20 health.")
            elif item == "Mana Potion":
                self.mana += 20
                if self.mana > 100:
                    self.mana = 100
                self.inventory.remove(item)
                print("You drink a Mana Potion and restore 20 mana.")
        else:
            print("You don't have that item.")

def earn_exp(self, exp):
        self.exp += exp
        print("You earn", exp, "experience points.")
        if self.exp >= 100:
            self.level_up()
            self.exp = 0
            print("Congratulations! You leveled up to level", self.level, "and gained 3 stat points.")

# Enemy class
class Enemy:
    def __init__(self, name, level, strength, health, defense, magic_type):
        self.name = name
        self.level = level
        self.strength = strength
        self.health = health
        self.defense = defense
        self.magic_type = magic_type

    def attack(self, player):
        damage = self.strength
        print("The", self.name, "attacks you and deals", damage, "damage.")
        player.take_damage(damage)

    def take_damage(self, damage):
        damage_taken = max(damage - self.defense, 0)
        self.health -= damage_taken
        print("The", self.name, "takes", damage_taken, "damage.")

# Weapon class
class Weapon:
    def __init__(self, name, damage, spell_damage):
        self.name = name
        self.damage = damage
        self.spell_damage = spell_damage

# Armor class
class Armor:
    def __init__(self, name, defense):
        self.name = name
        self.defense = defense

# Title class
class Title:
    def __init__(self, name):
        self.name = name

# Item class
class Item:
    def __init__(self, name):
        self.name = name

# Game setup
player = None

def create_save(player):
    with open("save_game.pkl", "wb") as file:
        pickle.dump(player, file)
    print("Game saved successfully.")

def start_new_game():
    name = input("Enter your player name: ")
    race = input("Enter your player race: ")
    magic_type = input("Enter your player magic type: ")
    player = Player(name, race, magic_type)
    print("New game started.")
    return player

def load_game():
    try:
        with open("save_game.pkl", "rb") as file:
            player = pickle.load(file)
        print("Game loaded successfully.")
        return player
    except FileNotFoundError:
        print("No save file found.")
        return None

def create_character():
    print("Welcome to Granjohiem Online!")
    name = input("Enter your character's name: ")
    print("Available races: Human, Elf, Dwarf, Beastperson, Monster")
    race = input("Select your character's race: ")
    print("Available magic types: Fire, Earth, Water, Admin, Air")
    magic_type = input("Select your character's magic type: ")
    player = Player(name, race, magic_type)
    return player

def start_new_game():
    print("New game started.")
    return create_character()


def show_main_menu():
    print("\n--- Main Menu ---")
    print("1. Start New Game")
    print("2. Load Game")
    print("3. Quit Game")

def show_stats_menu(player):
    print("\n--- Stats Menu ---")
    print("Name:", player.name)
    print("Race:", player.race)
    print("Magic Type:", player.magic_type)
    print("Level:", player.level)
    print("Experience Points:", player.exp)
    print("Stat Points:", player.stat_points)
    print("Health:", player.health)
    print("Mana:", player.mana)
    print("Stamina:", player.stamina)
    print("Damage:", player.damage)
    print("Defense:", player.defense)
    print("Magic Power:", player.magic_power)
    print("Endurance:", player.endurance)
    print("Hearts:", player.hearts)
    if player.equipped_weapon is not None:
        print("Equipped Weapon:", player.equipped_weapon.name)
    if player.equipped_armor is not None:
        print("Equipped Armor:", player.equipped_armor.name)
    if player.equipped_title is not None:
        print("Equipped Title:", player.equipped_title.name)



def show_battle_menu(player, enemy):
    print("\n--- Battle Menu ---")
    print("1. Attack")
    print("2. Cast Spell")
    print("3. Use Item")
    print("4. Run Away")
    print("5. Save Game")
    print("6. Quit Game")

def show_inventory_menu(player):
    print("\n--- Inventory Menu ---")
    print("1. Equip Weapon")
    print("2. Equip Armor")
    print("3. Use Item")
    print("4. Unequip Weapon")
    print("5. Unequip Armor")
    print("6. Go Back")

def show_title_menu(player):
    print("\n--- Title Menu ---")
    print("1. Equip Title")
    print("2. Unequip Title")
    print("3. Go Back")

def choose_spell(player):
    print("\n--- Choose Spell ---")
    spells = player.get_available_spells()
    for i, spell in enumerate(spells):
        print(f"{i+1}. {spell.name} (MP: {spell.mp_cost})")
    choice = int(input("Choose a spell: ")) - 1
    if choice < 0 or choice >= len(spells):
        print("Invalid spell choice. Try again.")
        return None
    return spells[choice]

class Enemy:
    def __init__(self, name, **kwargs):
        self.name = name
        self.level = kwargs.get('level', 1)
        self.attack_power = kwargs.get('attack_power', 0)
        self.health = kwargs.get('health', 0)
        self.defense_power = kwargs.get('defense_power', 0)
        self.magic_type = kwargs.get('magic_type', 'None')
    def is_alive(self):
        return self.health > 0

